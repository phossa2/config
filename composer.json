{
    "name": "phossa2/config",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/phossa2/config",
    "description": "A simple, easy to use, yet powerful configuration management library for PHP.",
    "keywords": [ "phossa2", "configuration", "configure", "config" ],
    "authors": [
        {
            "name": "Hong Zhang",
            "homepage": "https://github.com/phossa2"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "phossa2/shared": "^2.0.18"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "squizlabs/php_codesniffer": "2.*"
    },
    "autoload": {
        "psr-0": { "Phossa2\\Config\\": "src/" }
    },
    "scripts": {
        "test": "phpunit",
        "check": "phpcs --standard=psr2 src/",
        "format": "phpcbf --standard=psr2 src/"
    },
    "config": {
        "platform": {
            "php": "5.4.0"
        }
    },
    "provide": {
        "phossa2/config-implementation": "^2.0"
    },
    "replace": {
        "phossa/phossa-config": "*"
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "archive": {
        "exclude": [ "!vendor", "tests", "composer.lock" ]
    }
}
